<body>
  <div class="container">
    <hr>
    <button class="btn btn-success" onclick="first()">İstek At</button>
    <hr>
<div id="firma"></div>
<script>
    function first(){
        axios({
        method:'GET',
        url : 'http://localhost:3000/borsa'
    }).then((respons)=>{
        console.log(respons)
        $(document).ready(()=>{
            for(var i=0; i<respons.data.length; i++){

             $('#firma').append(`${respons.data[i][0]}`)
               console.log(respons.data)
            }
      })
    })
    }
</script>
